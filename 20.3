#include <iostream>
using namespace std;
int main()
{
	int k, i, n, * a, schet = 1, schet_end, tk = 1, schet_k, * b;
	cout << "Enter the array size N and the series number K\nN=";
	cin >> n;
	cout << "K=";
	cin >> k;
	if (k == 1)
		schet_k = 1;
	else
		schet_k = 0;
	a = new int[n + 1];
	b = new int[n + 1];
	for (i = 1; i <= n; i++) {
		cout << "\n Enter element array A#" << i << "=";
		cin >> a[i];
	}
	//
	for (i = 2; i <= n; i++) {
		if (a[i - 1] != a[i]) {
			tk++;
			if (tk == k) schet = i;
			schet_end = i;
		}
		if (tk == k) schet_k++;
	}
	int i2 = 0;
	for (i = 1; i < schet; i++)
		b[++i2] = a[i];
	for (i = schet_end; i <= n; i++)
		b[++i2] = a[i];
	for (i = schet + schet_k; i < schet_end; i++)
		b[++i2] = a[i];
	for (i = schet; i < schet + schet_k; i++)
		b[++i2] = a[i];
	for (i = 1; i <= n; i++)
		a[i] = b[i];
	//
	cout << "Array A:\n";
	for (i = 1; i <= n; i++)
	{
		cout << a[i] << " ";
	}
}
